{
  "name": "Micronets-Manager",
  "description": "Micronets Manager : FeathersJS + Vue.js web application framework",
  "version": "1.0.0",
  "homepage": "https://github.com/cablelabs/micronets-manager#readme",
  "private": true,
  "main": "src/",
  "keywords": [
    "feathers",
    "vuejs"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cablelabs/micronets-manager.git"
  },
  "author": "Ashwini Kadam <a.kadam@cablelabs.com>",
  "engines": {
    "node": "6.2.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "scripts": {
    "lint": "eslint-if-supported semistandard --fix",
    "start": " babel-node api/ && nodemon /api",
    "docker-build": "docker build -t code.cablelabs.com:4567/micronets/micronets-manager-backend .",
    "docker-publish": "docker login code.cablelabs.com:4567; docker push code.cablelabs.com:4567/micronets/micronets-manager-backend",
    "mocha": "mocha test/ --recursive --exit",
    "test": "npm run clean --silent && npm run lint --silent && npm run mocha --silent",
    "watch": "npm run clean --silent && cross-env NODE_ENV=development babel-node src/"
  },
  "semistandard": {
    "env": [
      "mocha"
    ]
  },
  "bugs": {
    "url": "https://github.com/cablelabs/micronets-manager/issues"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.6",
    "@feathersjs/authentication-jwt": "^2.0.1",
    "@feathersjs/authentication-local": "^1.2.1",
    "@feathersjs/configuration": "^1.0.2",
    "@feathersjs/errors": "^3.3.0",
    "@feathersjs/express": "^1.2.3",
    "@feathersjs/feathers": "^3.1.7",
    "@feathersjs/socketio": "^3.2.2",
    "axios": "^0.18.0",
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "body-parser": "^1.16.0",
    "bootstrap": "^4.0.0-alpha.6",
    "compression": "^1.7.2",
    "connect-history-api-fallback": "^1.3.0",
    "cors": "^2.8.4",
    "feathers-mongodb": "^3.2.0",
    "feathers-mongoose": "^6.1.2",
    "helmet": "^3.12.1",
    "ip-address": "^5.8.9",
    "mongodb": "^3.0.10",
    "mongoose": "^5.2.1",
    "nodemon": "^1.17.5",
    "ramda": "^0.25.0",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.1.1",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "browser-sync": "^2.18.6",
    "browser-sync-webpack-plugin": "^1.1.3",
    "chai": "^4.1.2",
    "cross-env": "^3.1.4",
    "css-loader": "^0.25.0",
    "del-cli": "^0.2.1",
    "eslint-if-supported": "^1.0.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "mocha": "^5.2.0",
    "postcss-flexbugs-fixes": "^2.1.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "semistandard": "^9.2.1",
    "vue": "^2.1.10",
    "vue-feathers": "^0.1.0",
    "vue-loader": "^10.0.0",
    "vue-router": "^2.2.0",
    "vue-template-compiler": "^2.1.0",
    "vuex": "^2.1.1",
    "vuex-router-sync": "^4.1.2",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-merge": "^2.4.0"
  }
}
